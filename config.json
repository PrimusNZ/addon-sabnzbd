{
  "name": "SABnzbd",
  "version": "0.1.0",
  "slug": "sonarr",
  "legacy": false,
  "init": false,
  "maintenance": {
    "github_release": "https://github.com/linuxserver/docker-sabnzbd",
    "version_regex": "(\\d+\\.\\d+\\.\\d+\\.\\d+)-(ls\\d+)"
  },
  "description": "PVR for usenet and bittorrent users",
  "url": "https://github.com/PrimusNZ/addon-sabnzbd",
  "startup": "services",
  "arch": [
    "aarch64",
    "amd64",
    "armhf"
  ],
  "ports": {
    "8080/tcp": 8080
  },
  "ports_description": {
    "8080/tcp": "web interface"
  },
  "map": [
    "config:rw",
    "share:rw",
    "media:rw",
    "ssl"
  ],
  "webui": "http://[HOST]:[PORT:8080]",
  "boot": "auto",
  "options": {
    "PUID": 1001,
    "PGID": 1001
  },
  "schema": {
    "PUID": "int",
    "PGID": "int",
    "TZ": "str?"
  },
  "backup_exclude": [
    "**/Backups/*",
    "**/logs/*",
    "**/MediaCover/*"
  ],
  "image": "ghcr.io/primusnz/hassio-addons/{arch}-sonarr"
}
